{
    "name": "aymericcucherousset/telegram-bot",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Aymericcucherousset\\TelegramBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Fake\\": "tests/Fake/"
        }
    },
    "authors": [
        {
            "name": "aymericcucherousset",
            "email": "aymeric.cucherousset@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.1",
        "psr/log": "^3.0",
        "psr/http-factory": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.93"
    },
    "scripts": {
        "lint": "vendor/bin/php-cs-fixer fix && vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    }
}
